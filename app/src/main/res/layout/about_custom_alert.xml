<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:background="@color/main_background">

<ImageView
    android:id="@+id/wellness"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:src="@drawable/mon_child"
    android:layout_marginTop="8dp"
    android:layout_marginLeft="8dp"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Wellness Baby"
        android:textSize="25dp"
        android:layout_alignParentTop="true"
        android:layout_toRightOf="@+id/wellness"
        android:layout_toEndOf="@+id/wellness"
        android:layout_marginLeft="45dp"
        android:layout_marginTop="40dp"
        android:textStyle="bold|italic"
        android:id="@+id/textView5" />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="version 1.0"
        android:textSize="16dp"
        android:layout_below="@+id/textView5"
        android:layout_alignLeft="@+id/textView5"
        android:layout_alignStart="@+id/textView5"
        android:layout_marginLeft="37dp"
        android:layout_marginStart="37dp"
        android:id="@+id/textView6" />
    <View
        android:id="@+id/view"
        android:layout_width="wrap_content"
        android:layout_height="4dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignRight="@+id/textView5"
        android:layout_alignEnd="@+id/textView5"
        android:layout_below="@+id/wellness"/>

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:layout_below="@+id/view"
        android:layout_marginTop="5dp"
        android:layout_margin="10dp">

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textSize="25dp"
            android:text="Samsung Mobile App Training 2016"
            android:textStyle="bold|italic"/>

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Team LM-1615    CU(Sittwe)"
            android:layout_marginLeft="60dp"
            android:layout_marginTop="5dp"
            android:textSize="20dp"
            android:textStyle="bold"/>



        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:layout_marginLeft="100dp"
            android:paddingTop="8dp"
            android:text="-Mg Thin Myat Nyein"
            android:drawableRight="@drawable/dfsafsd"
            android:textSize="20dp"/>

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:layout_marginLeft="100dp"
            android:text="-Mg Myo Min Thu"
            android:drawableRight="@drawable/fdas"
            android:textSize="18dp"/>

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:layout_marginLeft="100dp"
            android:text="-Mg Aung Naing Tun"
            android:drawableRight="@drawable/aungnaing"
            android:textSize="18dp"/>

    </LinearLayout>

</RelativeLayout>